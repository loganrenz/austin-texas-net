{
  "name": "austin-texas-net",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "doppler run --project austin-texas-net --config dev -- nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "npm_config_loglevel=error nuxt typecheck",
    "deploy": "nuxt build && node scripts/patch-cf-process-proxy.mjs && wrangler deploy",
    "lint": "eslint app/ server/ --no-error-on-unmatched-pattern",
    "test:e2e": "playwright test",
    "quality": "pnpm run lint && pnpm run typecheck",
    "prepare": "husky",
    "postbuild": "node scripts/patch-cf-process-proxy.mjs",
    "dev:kill": "pkill -f 'nuxt/bin/nuxt.mjs dev' ;pkill -f 'nuxt.*austin-texas-net' ; pkill -f 'pnpm.*austin-texas-net.*dev' ; pkill -f workerd ; echo 'Killed all austin-texas-net dev processes'",
    "logs": "wrangler tail austin-texas-net"
  },
  "dependencies": {
    "@nuxt/content": "^3.11.2",
    "@nuxt/ui": "^4.3.0",
    "@resvg/resvg-js": "^2.6.2",
    "@resvg/resvg-wasm": "^2.6.2",
    "better-sqlite3": "^12.6.2",
    "chart.js": "^4.5.1",
    "drizzle-orm": "^0.45.1",
    "jose": "^6.1.3",
    "nuxt": "^4.3.0",
    "nuxt-schema-org": "^5.0.10",
    "posthog-js": "^1.342.1",
    "react": "^19.2.4",
    "satori": "^0.15.2",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.27",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.4",
    "workers-og": "^0.0.27",
    "yoga-wasm-web": "^0.3.3",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250620.0",
    "@iconify-json/lucide": "^1.2.90",
    "@nuxt/eslint": "^1.15.1",
    "@nuxt/fonts": "^0.13.0",
    "@nuxtjs/robots": "^5.7.0",
    "@nuxtjs/sitemap": "^7.6.0",
    "@playwright/test": "^1.58.2",
    "@types/node": "^22.15.0",
    "@types/react": "^19.2.14",
    "eslint": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nitro-cloudflare-dev": "^0.2.2",
    "prettier": "^3.8.1",
    "wrangler": "^4.20.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ],
    "overrides": {
      "@cloudflare/unenv-preset": "2.12.1"
    }
  },
  "volta": {
    "node": "22.22.0"
  }
}
